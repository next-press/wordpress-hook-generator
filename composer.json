{
    "name": "next-press/wordpress-hook-generator",
    "description": "Parses the hooks and filters on our plugin and generate a markdown file with the contents",
    "license": "GPL",
    "bin": [
        "bin/wordpress-hook-generator"
    ],
    "authors": [
        {
            "name": "Arindo Duque",
            "email": "arindo@wpultimo.com"
        }
    ],
    "require": {
        "next-press/wordpress-hook-parser": "dev-master",
        "nategood/commando": "^0.4.0",
        "next-press/phpdoc-parser": "dev-master",
        "composer/installers": "~1.0",
        "phpdocumentor/reflection": "~3.0",
        "erusev/parsedown": "~1.7",
        "scribu/lib-posts-to-posts": "dev-master@dev",
        "scribu/scb-framework": "dev-master@dev",
        "psr/log": "~1.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "next-press/wordpress-hook-parser",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/next-press/wordpress-hook-parser.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "next-press/phpdoc-parser",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/next-press/phpdoc-parser.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "vendor/next-press/phpdoc-parser/lib/runner.php",
            "vendor/next-press/phpdoc-parser/lib/template.php"
        ],
        "classmap": [
            "vendor/next-press/wordpress-hook-parser/src",
            "vendor/next-press/phpdoc-parser"
        ]
    }
}